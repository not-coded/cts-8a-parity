    ],
    "acl": ["net.notcoded.cts8a_parity.AscendClient"]
  },
  "mixins": [
    "cts8a_parity.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.21",
    "minecraft": "1.16.x",
