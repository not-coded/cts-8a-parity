  "authors": [
    "NotCoded"
  ],
  "contributors": [
    "Alexandra"
  ],
  "contact": {
    "homepage": "https://notcoded.is-a.dev",
    "sources": "https://github.com/not-coded/cts-8a-parity"
  },

  "license": "GPL-3.0",
  "icon": "assets/cts8a_parity/icon.png",

  "environment": "client",
  "entrypoints": {
    "client": [
